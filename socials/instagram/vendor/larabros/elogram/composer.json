{
    "name": "larabros/elogram",
    "description": "An \"eloquent\" way of accessing Instagram's API",
    "keywords": [
        "larabros",
        "instagram",
        "sdk"
    ],
    "homepage": "https://github.com/larabros/elogram",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Khan",
            "email": "contact@hassankhan.me",
            "homepage": "http://hassankhan.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "guzzlehttp/guzzle": "^6.0.1",
        "league/oauth2-instagram": "^0.2.0",
        "hassankhan/config": "^0.10.0",
        "illuminate/support": "^5.1.0",
        "league/container": "^2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "vlucas/phpdotenv": "^2.2",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^2.5",
        "sphpdox/sphpdox": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Larabros\\Elogram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larabros\\Elogram\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "docs": "sphpdox process -o docs/ -t \"API Reference\" \"Larabros\\Elogram\" src/ && sphinx-build docs/ docs/build/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
